<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0D6EFD"
    tools:context=".kolaj3x2">

    <FrameLayout
        android:id="@+id/frameLayout2x3"
        android:layout_width="390dp"
        android:layout_height="600dp"
        android:background="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imgback6"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <ImageView
            android:id="@+id/img1_6"
            android:layout_width="191.5dp"
            android:layout_height="196dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="1dp"
            app:srcCompat="@drawable/ic_action_name" />

        <ImageView
            android:id="@+id/img2_6"
            android:layout_width="191.5dp"
            android:layout_height="196dp"
            android:layout_marginStart="197.5dp"
            android:layout_marginTop="1dp"
            app:srcCompat="@drawable/ic_action_name" />

        <ImageView
            android:id="@+id/img3_6"
            android:layout_width="191.5dp"
            android:layout_height="196dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="202dp"
            app:srcCompat="@drawable/ic_action_name" />

        <ImageView
            android:id="@+id/img4_6"
            android:layout_width="191.5dp"
            android:layout_height="196dp"
            android:layout_marginStart="197.5dp"
            android:layout_marginTop="202dp"
            app:srcCompat="@drawable/ic_action_name" />

        <ImageView
            android:id="@+id/img5_6"
            android:layout_width="191.5dp"
            android:layout_height="196dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="403dp"
            app:srcCompat="@drawable/ic_action_name" />

        <ImageView
            android:id="@+id/img6_6"
            android:layout_width="191.5dp"
            android:layout_height="196dp"
            android:layout_marginStart="197.5dp"
            android:layout_marginTop="403dp"
            app:srcCompat="@drawable/ic_action_name" />
    </FrameLayout>

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/foto_raf_se_iniz"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout2x3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/saveimg6"
        android:layout_width="48dp"
        android:layout_height="61dp"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/frameLayout2x3"
        app:layout_constraintEnd_toStartOf="@+id/textView2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/btnsave6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:text="Kaydet"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2x3"
        app:layout_constraintVertical_bias="0.542" />

    <Button
        android:id="@+id/btnarkaplan2x3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Arkaplan ayarla"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnsave6"
        app:layout_constraintHorizontal_bias="0.155"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/frameLayout2x3"
        app:layout_constraintVertical_bias="0.542" />

</androidx.constraintlayout.widget.ConstraintLayout>